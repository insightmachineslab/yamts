<?xml version='1.0' encoding='utf-8'?>
<widget id="com.insightmachines.yamts" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>YAMTS-Control</name>
    <description>
        Remote control panel for the YAMTS device
    </description>
    <author email="greg@insightmachines.com" href="http://insightmachines.com">
        Grzegorz Ho≈Çdys
    </author>
    <icon src="www/img/icon-72x72.png" />
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
